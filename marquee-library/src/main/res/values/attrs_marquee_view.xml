<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MarqueeView">
        <attr name="marquee_direction" format="enum">
            <enum name="startToEnd" value="0" />
            <enum name="topToBottom" value="1" />
            <enum name="endToStart" value="2" />
            <enum name="bottomToTop" value="3" />
        </attr>
        <attr name="marquee_anim_duration" format="integer" />
    </declare-styleable>
</resources>
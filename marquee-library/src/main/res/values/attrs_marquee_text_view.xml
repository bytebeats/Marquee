<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MarqueeTextView">
        <attr name="repeat_mode" format="enum">
            <enum name="forever" value="0" />
            <enum name="once" value="1" />
        </attr>
        <attr name="forever_mode" format="enum">
            <enum name="shortStay" value="0" />
            <enum name="appending" value="1" />
        </attr>
        <attr name="short_stay_delay" format="integer" />
        <attr name="start_delay" format="integer" />
        <attr name="marquee_speed" format="integer" />
        <attr name="reset_x" format="boolean" />
        <attr name="tap_to_pause" format="boolean" />
    </declare-styleable>
</resources>